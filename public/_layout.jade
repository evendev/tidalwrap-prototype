!!!5
html
  head
    meta(charset='utf-8')
    title= site.name
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    link(rel='stylesheet', href='css/app.css')
    link(rel='stylesheet', href='css/animate.css') script(src='//cdnjs.cloudflare.com/ajax/libs/prefixfree/1.0.7/prefixfree.min.js')
    //- script(src='js/modernizr.js')
    //if lt IE 9
      script(src='//oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='//oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js')
  body

    != partial('_header')

    != partial('_nav')

    .site-wrap
      .site.site-main
        != yield

    //.site-map
      #map

    != partial('_footer')

    script.
      window._tidalWrapLatitude = #{ company.latitude };
      window._tidalWrapLongitude = #{ company.longitude };
    script(src='//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js')
    script(src='js/bootstrap.min.js')
    script(src='//maps.google.com/maps/api/js?sensor=true')
    script(src='js/gmaps.js')
    script(src='js/wow.js')
    script(src='js/app.js')

    include _google-analytics.html
